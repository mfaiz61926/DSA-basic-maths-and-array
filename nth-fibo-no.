#include<bits/stdc++.h>

using namespace std;

 

int main()

{

       int arr[100];

       arr[0]=0;
                                                                                                                                                                                                 
       arr[1]=1;

       int x;

       cin>>x;

       for(int i=2;i<=x;i++){

               arr[i]=arr[i-1]+arr[i-2];

       }

       cout<<arr[x];

 

       

}
